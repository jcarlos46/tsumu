{
  "name": "Tsumu Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#1e1e1e",
    "editor.foreground": "#d4d4d4",
    "editor.lineHighlightBackground": "#2a2a2a",
    "editorCursor.foreground": "#ffffff",
    "editor.selectionBackground": "#264f78",
    "editor.inactiveSelectionBackground": "#3a3d41"
  },
  "tokenColors": [
    {
      "scope": "keyword.control.tsumu",
      "settings": {
        "foreground": "#569cd6",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "support.function.stack.tsumu",
      "settings": {
        "foreground": "#dcdcaa"
      }
    },
    {
      "scope": "keyword.operator.logical.tsumu",
      "settings": {
        "foreground": "#c586c0"
      }
    },
    {
      "scope": "constant.numeric.tsumu",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "string.quoted.double.tsumu",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "meta.brackets.list.tsumu",
      "settings": {
        "foreground": "#4ec9b0"
      }
    },
    {
      "scope": "meta.brackets.expr.tsumu",
      "settings": {
        "foreground": "#d7ba7d"
      }
    },
    {
      "scope": "comment.line.double-dash.tsumu",
      "settings": {
        "foreground": "#6a9955",
        "fontStyle": "italic"
      }
    }
  ]
}